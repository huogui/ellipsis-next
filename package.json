    "release": "standard-version"
  },
  "dependencies": {
    "sass": "^1.56.1",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@huogui/eslint-config": "^1.1.30",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.27.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-style-inject": "^0.0.1",
    "vue-tsc": "^1.0.9"
  }
}
