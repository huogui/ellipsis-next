{
  "name": "vue-next-ellipsis",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/vue-next-ellipsis.umd.cjs",
  "module": "./dist/vue-next-ellipsis.js",
  "exports": {
    ".": {
      "import": "./dist/vue-next-ellipsis.js",
      "require": "./dist/vue-next-ellipsis.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/huogui/vue-next-ellipsis"
  },
  "homepage": "https://huogui.github.io/vue-next-ellipsis",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vue-tsc --noEmit && vite build",
